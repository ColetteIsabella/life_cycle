<%= form_for(@fellow) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :twitter %><br />
    <%= f.text_field :twitter %>
  </p>

  <p>
    <%= f.label :facebook %><br />
    <%= f.text_field :facebook %>
  </p>

  <p>
    <%= f.label :linkedin, "LinkedIn" %><br />
    <%= f.text_field :linkedin %>
  </p>

  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>

  <%= f.submit %>
<% end %>
